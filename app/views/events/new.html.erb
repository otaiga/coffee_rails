<h1>Create a new Event</h1>

<%= form_for @event, :url => events_path, :method => :post do |f| %>
  <%= f.label :title%><br/>
  <%= f.text_field :title %><br />
  <%= f.label :description %><br/>
  <%= f.text_area :description %><br />
  <%= f.submit %>
<% end %>
<br>
<%= link_to "Back Event View", events_path %>