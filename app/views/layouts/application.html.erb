<!DOCTYPE html>
<html>
<head>
  <title>Coffee</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <% if current_user %>
      <%= link_to "Home", root_path %>
      <%= link_to "Sign Out", destroy_user_session_path, confirm: 'Are you sure you want to logout?', :method => :delete %>
    <% end %>
  </nav>

<%= yield %>

</body>
</html>
